    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.6",
    "@types/file-type": "^10.9.1",
    "@types/jsonfile": "^6.1.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "connect-sqlite3": "^0.9.13",
    "crypto": "^1.0.1",
