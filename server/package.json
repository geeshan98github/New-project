  "scripts": {
    "dev": "yarn with-env ts-node-dev --respawn --transpile-only --ignore-watch node_modules utils/server.ts",
    "build": "tsc",
    "start": "node dist/boot/server.js",
    "with-env": "dotenv -e ../.env --",
